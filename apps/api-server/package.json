{
  "name": "@template/api-server",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "build:clean": "pnpm clean:build && pnpm build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:clean": "pnpm clean:test && pnpm test",
    "clean": "pnpm clean:build && pnpm clean:test",
    "clean:build": "rimraf ./dist ./tsconfig*.tsbuildinfo",
    "clean:test": "jest --clearCache && rimraf ./coverage",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\""
  },
  "dependencies": {
    "@keyv/redis": "^5.1.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@template/core": "workspace:*",
    "cache-manager": "^7.2.0",
    "fastify": "^5.5.0",
    "nest-winston": "^1.10.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@template/eslint-config": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
